<h1><%= @company.name %></h1>


<table>
  <tbody>
    <tr>
      <th>Project</th>
      <th>Company</th>
    </tr>
    <% @company.projects.each do |project| %>
      <tr>
        <td><%= link_to project, project %></td>
        <td><%= link_to project.company.name, project.company %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<% content_for :aside do %>
  <% render 'sidebar' %>
<% end %>